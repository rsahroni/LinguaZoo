{
  "bumpFiles": [
    {
      "filename": "package.json",
      "type": "json"
    },
    {
      "filename": "app.json",
      "type": "json",
      "versioner": "json"
    },
    {
      "filename": "README.md",
      "updater": {
        "readVersion": "version-([0-9]+\\.[0-9]+\\.[0-9]+)-blue\\.svg",
        "writeVersion": "version-{{currentTag}}-blue.svg"
      }
    }
  ]
}