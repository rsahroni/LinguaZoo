{
  "expo": {
    "name": "LinguaZoo",
    "slug": "LinguaZoo",
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./assets/linguazoo-icon-v1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/linguazoo-icon-v1.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6F61"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/linguazoo-icon-v1.png",
        "backgroundColor": "#FF6F61"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rhsahroni.LinguaZoo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-splash-screen"
    ],
    "extra": {
      "eas": {
        "projectId": "7467fec5-bfe7-4eea-b90e-8305f08da303"
      }
    }
  }
}
